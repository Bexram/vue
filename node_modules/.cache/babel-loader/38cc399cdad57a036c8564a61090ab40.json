EAAA,QAAA,oBACA,UAAA,CAAA,CACA,OADA,CAAA,CADA;AAtCA,CAAA","sourcesContent":["<template>\n  <div class=\"v-users-list\" >\n    <v-change-popup v-show=\"showPopUp !== false\" />\n    <tableUsers/>\n    </div>\n</template>\n\n<script>\nimport tableUsers from './v-table-users'\nimport {mapActions,mapGetters} from 'vuex'\nimport vChangePopup from './v-change-popup'\nimport { bus } from './bus.js'\n\nexport default {\n  name: 'v-users-list',\n  components: {\n    tableUsers,\n    vChangePopup\n  },\n  props: {},\n  data() {\n    return {\n      changeUser: null,\n      showPopUp: false,\n    }\n  },\n  methods: {\n    ...mapActions([\n      'GET_USERS_FROM_API'\n    ]),\n    change(index){\n    this.changeUser = index;\n    console.log('changeUser=',this.changeUser);  \n    },\n    allowNewUser(data){ \n    this.showPopUp = data;\n    }\n  },\n  \n  beforeMount(){\n\n  },\n\n  mounted() {\n    this.GET_USERS_FROM_API();\n    bus.$on('sendIndex',this.change);\n    bus.$on('addNewUser',this.allowNewUser);   \n    bus.$on('editUser',this.allowNewUser); \n    bus.$on('closeWindow',this.allowNewUser); \n\n  },\n  computed:{\n    ...mapGetters([\n      'USERS'\n    ])\n  }\n}\n</script>\n\n<style lang=\"css\">\ntd,table,tr{\nborder: 1px solid black;  \n}\n\ntd{\npadding: 0 5px;  \n}\n\n.v-users-list{\ndisplay: flex;\njustify-content: center;  \nflex-direction: column;\nmax-width: 100%;\nwidth: 100%;\nposition: absolute;\nleft: 0px;\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}