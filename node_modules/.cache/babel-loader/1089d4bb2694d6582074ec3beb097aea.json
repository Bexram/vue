ODUCTS_TO_STATE',response.data);\n        console.log(response,'user was added');\n        return response;\n    }) \n    .catch((error) => {\n        console.log(error);\n        return error;\n    });\n    },\n    ADD_USER_TO_API({commit},user){\n        console.log('user is ',user);\n        return axios({\n            method: 'POST',\n            url: 'http://25.50.155.62:8000/users/',\n            data: user,\n            headers: {\n                \"content-type\": \"application/json\"\n              },\n        })\n    .then((response) =>{\n        commit('ADD_USER_TO_STATE',response.data);\n        console.log(user,'user was added');\n        return response;\n    })\n    .catch((error) => {\n        console.log(error);\n        return error;\n    });  \n    },\n    CHANGE_USER_IN_API({commit},changingParameters){\n        return axios ({\n            method: 'PUT',\n            url: 'http://25.50.155.62:8000/users/'+changingParameters.id+'/',\n            data: changingParameters,\n            headers: {\n                \"content-type\": \"application/json\"\n              },\n        })\n        .then((response) =>{\n            console.log('response is ',response)\n            commit('SET_PRODUCTS_TO_STATE',response.data);\n            return response;\n        })\n        .catch((error) => {\n            console.log(error);\n            return error;\n        });  \n},\n        GET_AUTH_FROM_API({commit},login_data){\n            return axios ({\n                method: \"POST\",\n                url: 'http://25.50.155.62:8000/auth/',\n                data: {\n                    username: login_data.username,\n                    password: login_data.password\n                }\n              })\n              .then((response) => {\n                commit('SET_AUTH_TO_STATE',response.data);\n                Vue.$cookies.set(\"access\",response.data.access,1);\n              })\n              .catch((error) => {\n                console.log(error);\n                console.log(login_data);\n                \n              })\n        }\n},\ngetters:{\n    USERS(state){\n        return state.users;\n    },\n},\n});\n\nexport default store\n"]}]}