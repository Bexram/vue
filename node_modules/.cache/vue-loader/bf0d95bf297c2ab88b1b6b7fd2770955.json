nput v-model=\"newUser.email\"></p>\n    <p>Userprofile.position:<input v-model=\"newUser.position\"></p>\n    <p>Userprofile.thirdname:<input v-model=\"newUser.thirdname\"></p>\n    <p>Telephone:<input v-model=\"newUser.telephone\"></p>\n    <p>Birthday:<input v-model=\"newUser.birthday\"></p>\n    <p>Passport:<input v-model=\"newUser.passport\"></p>\n    <p>User:<input v-model=\"newUser.user\"></p>\n    <button @click=\"toAddNewUser\">Добавить</button>\n  </div>\n  </div>\n</template>\n\n<script>\nimport { bus } from './bus.js'\nimport axios from 'axios'\n\nexport default {\n  name: 'v-change-popup',\n  components: {\n  },\n  props: {\n    },\n  data() {\n    return {\n      users:{},\n      createNewUser: false,\n      userId: null,\n      newUser:{\n      \"username\": null,\n      \"first_name\": null,\n      \"password\": null,\n      \"last_name\": null,\n      \"email\": null,\n      \"is_active\": null,\n        \"thirdname\": null,\n        \"position\": null,\n        \"telephone\": null,\n        \"birthday\": null,\n        \"passport\": null,\n        \"user\": null\n      },\n    }\n  },\n\n  methods:{\n    getUsersFromApi(){\n      axios .get ('http://25.58.181.47:8000/users/')\n        .then((response) =>{\n          this.users = response.data;\n        console.log(response,'users got');\n      })\n      .catch((error) => {\n                console.log(error);  \n                return error;\n    })\n    },\n    getIndex(index){\n      this.userId = index;\n      console.log('Popup get', this.userId);\n    },\n    toCreateNewUser(data){\n    this.createNewUser = data;  \n    },\n    toAddNewUser(){\n      let user = this.newUser;\n      console.log(user);\n      /*axios .post('http://25.58.181.47:8000/users/',user,headers) */\n      axios({\n      method: 'post',\n      url: 'http://25.58.181.47:8000/users/',\n      data: {\n      user  \n      },\n     headers: {\n    \"content-type\": \"application/json\"\n  }\n  })\n      .then(response => {\n        return response;\n      })\n      .catch((error) => {\n      console.log(error);  \n      return error;\n        });\n      bus.$emit('sendNewUserToTable',this.newUser);\n      bus.$emit('addNewUser',false);\n    },\n    closePopup(){\n    bus.$emit('closeWindow',false);  \n    },\n    \n  },\n  \n  mounted() {\n    this.getUsersFromApi();\n    bus.$on('sendIndex',this.getIndex); \n    bus.$on('addNewUser',this.toCreateNewUser);\n    \n  },\n}\n</script>\n\n<style lang=\"css\" scoped>\n.v-change-popup{\n display:block;\n width: 500px;\n z-index:999; \n border: 1px solid black;\n background: white; \nmargin: 20px auto 0 auto;\npadding: 20px;\nposition: relative;  \n}\n\n.close-button{\nposition: absolute;\nright: 10px;\ntop: 10px;  \n}\n</style>\n"]}]}